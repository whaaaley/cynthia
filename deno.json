{
  "name": "cynthia",
  "version": "0.0.1",
  "exports": {
    ".": "./src/lib/mod.ts",
    "./cli": "./src/cli.ts"
  },
  "bin": {
    "cyn": "./bin/cyn.ts"
  },
  "tasks": {
    "dev": "deno run --allow-all ./bin/cyn.ts",
    "build": "deno compile --allow-all --output cyn ./bin/cyn.ts",
    "install": "deno install --global --allow-all --name cyn ./bin/cyn.ts",
    "test": "deno test --allow-all"
  },
  "fmt": {
    "lineWidth": 240,
    "semiColons": false,
    "singleQuote": true
  },
  "imports": {
    "@openai/openai": "jsr:@openai/openai@^4.81.0",
    "@std/cli": "jsr:@std/cli@^1.0.11",
    "@std/expect": "jsr:@std/expect@^1.0.12",
    "@std/fs": "jsr:@std/fs@^1.0.10",
    "@std/path": "jsr:@std/path@^1.0.8",
    "@std/testing": "jsr:@std/testing@^1.0.9",
    "find-up-simple": "npm:find-up-simple@^1.0.1",
    "zod": "npm:zod@^3.24.1"
  }
}
